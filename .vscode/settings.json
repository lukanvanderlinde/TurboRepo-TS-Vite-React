{
    "files.exclude": {
        "**/.git": false,
        "**/.svn": false,
        "**/.hg": false,
        "**/CVS": false,
        "**/.DS_Store": false,
        "**/Thumbs.db": false,
        "**/.firebaserc": false,
        "**/.turbo": false,
        "**/.firebase": false,
        "**/.vscode": false,
        "**/.storybook": false,
        "**/**.log": false,
        "**/yarn.lock": false,
        "**/node_modules": false,
        "**/dist": false,
        "**/emulator": false,
        "**/routeTree.gen.ts": false,
        "**/*.d.ts": false
    },
    "material-icon-theme.files.associations": {
        "storage.rules": "firebase",
        "database.rules.json": "firebase",
